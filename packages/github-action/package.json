{
  "name": "@samithahansaka/perflock-action",
  "version": "0.1.0",
  "description": "GitHub Action for @samithahansaka/perflock - enforce performance contracts in CI",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "dev": "ncc build src/index.ts -o dist --source-map --watch",
    "test": "echo \"No tests yet\"",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "github-action",
    "react",
    "performance",
    "testing",
    "perflock"
  ],
  "author": "samithahansaka",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/samithahansaka/perflock"
  },
  "homepage": "https://github.com/samithahansaka/perflock#readme",
  "dependencies": {
    "@actions/artifact": "^5.0.2",
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.0",
    "@actions/github": "^6.0.0",
    "@actions/io": "^1.1.0",
    "@samithahansaka/perflock": "*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vercel/ncc": "^0.38.0",
    "typescript": "^5.3.0"
  }
}
